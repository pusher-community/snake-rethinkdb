<html>
<head>

  <!-- Latest compiled and minified CSS -->
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css">

  <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
  <script src="//js.pusher.com/2.2/pusher.min.js"></script>
  <script src="http://code.highcharts.com/highcharts.js" defer></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/underscore.js/1.8.3/underscore-min.js"></script>

  <link rel="stylesheet" href="bootstrap.min.css">
  <link rel="stylesheet" type="text/css" href="bootstrap-overrides.css">
  <link rel="stylesheet" type="text/css" href="styles.css">
  <script src="bootstrap.min.js"></script>

  <script src="snake.js"></script>
  <script src="app.js"></script>

  <title></title>
</head>
<body>

  <div class="container">

    <div class="inner">
      <div class="row">
        <h1 class="text-center page-heading">The Snake</h1>
        <h4 class="text-center page-subheading">Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod
        tempor incididunt ut labore et dolore magna aliqua.</h4>
      </div>

      <div class="row">
        <div class="col-md-6">

          <% if @current_player %>
          <h2>Welcome back, <span id="player-name"><%= @current_player["name"] %></span></h2>

          <button class="submit-notification btn btn-lg btn-clear btn-wide btn-default" id="start-playing">Click here to start playing</button>
          <% else %>

          <input type="text" id="enter-name" placeholder="Please enter your name">

          <% end %>

          <canvas id="canvas" width="450" height="450"></canvas>

        </div>

        <div class="col-md-6">

          <div class="col-md-6">
            <ul id="json-feed">
            </ul>
          </div>

          <div class="col-md-6">
            <ul id="high-scores-list">
            </ul>
          </div>

        </div>
      </div>

      <div class="row">
        <div id="graph" class="epoch category40" style="width:100%;height:400px"></div>
      </div>
    </div>

  </div>

</body>
</html>